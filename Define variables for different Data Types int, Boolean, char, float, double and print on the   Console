public class MyClass{
    public static void main(String args[]) {
        
        int a = 5;
        Boolean b = true;
        char c = 'C';
        float d = 5.5f; 
        double e = 5.555;
        System.out.println("int - "+a);
        
        System.out.println("Boolean -"+b);
        
        System.out.println("Char -"+c);
        System.out.println("float -"+d);
        System.out.println("double -"+e);
      
      
    }
}
