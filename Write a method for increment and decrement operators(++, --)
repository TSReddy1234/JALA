public class MyClass {
    public static void main(String args[]) {
      int x = 1;
      int y = 1;
      
     
      System.out.println("Orignal(x) : " + x);
      System.out.println("Incremented(x) : "+ ++x);
      System.out.println("Orignal(y) : " + y);
      System.out.println("Decremented(y) : "+ --y);
      
      
    }
}
